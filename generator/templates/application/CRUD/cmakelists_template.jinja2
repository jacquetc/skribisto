find_package(Qt6 COMPONENTS Core  REQUIRED)

set(PROJECT_NAME {{ application_name }}-application-feature-{{ feature_snake_name }})

set(SRC

    {% for file in files %}
    {{ file }}
    {%- endfor %}
)
qt_add_library(${PROJECT_NAME} STATIC ${SRC})

target_include_directories(${PROJECT_NAME} PUBLIC "${CMAKE_CURRENT_SOURCE_DIR}")

add_dependencies(${PROJECT_NAME} {{ application_name }}-contracts)
target_link_libraries(${PROJECT_NAME} PUBLIC {{ application_name }}-contracts)

target_link_libraries(${PROJECT_NAME}
    PRIVATE Qt6::Core
)